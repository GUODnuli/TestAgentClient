# 测试引擎配置

[testengine]
# 默认引擎：requests / httprunner
default_engine = "requests"

# 请求超时时间（秒）
timeout = 30

# 最大并发数
max_parallel = 20

# 失败自动重试
retry_on_failure = true

# 最大重试次数
max_retries = 3

# 协议适配器配置（用于自定义协议扩展）
[testengine.protocol_adapter]
# 适配器类路径（可选）
# adapter_class = "adapters.custom.MyProtocolAdapter"

# 适配器配置（可选）
[testengine.protocol_adapter.adapter_config]
# encryption = "AES256"
# auth = "custom_token"
